// prisma/schema.prisma

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Talent {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  socialMedia String?
  content   String[]
  role      String   @default("talent")
  createdAt DateTime @default(now())
}

